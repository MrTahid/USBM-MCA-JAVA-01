package babru1;

import java.util.Scanner;

public class pattern {
    public static void main(String[] args) {
        Scanner se=new Scanner(System.in);
        int n=se.nextInt();
        for (int i=n;i>0;i--){
            for (int j=i;j>0;j--){
                System.out.print("*");

            }
            System.out.print("\n");
        }
    }
}
